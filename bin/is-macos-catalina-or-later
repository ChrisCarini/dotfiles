#!/usr/bin/env bash
# 19 = Catalina
if [[ "$OSTYPE" =~ ^darwin19 ]]; then
  exit 0
# 20 = Big Sur
elif [[ "$OSTYPE" =~ ^darwin20 ]]; then
  exit 0
# 21 = Monterey
elif [[ "$OSTYPE" =~ ^darwin21 ]]; then
  exit 0
# 22 = Ventura
elif [[ "$OSTYPE" =~ ^darwin22 ]]; then
  exit 0
# 23 = Sonoma
elif [[ "$OSTYPE" =~ ^darwin23 ]]; then
  exit 0
else
  exit 1
fi
